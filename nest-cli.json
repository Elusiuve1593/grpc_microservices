{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/grpc_microservices/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/grpc_microservices/tsconfig.app.json"
  },
  "plugins": [
    {
      "name": "ts-proto",
      "options": {
        "env": "node"
      }
    }
  ],
  "monorepo": true,
  "root": "apps/grpc_microservices",
  "projects": {
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "grpc_microservices": {
      "type": "application",
      "root": "apps/grpc_microservices",
      "entryFile": "main",
      "sourceRoot": "apps/grpc_microservices/src",
      "compilerOptions": {
        "tsConfigPath": "apps/grpc_microservices/tsconfig.app.json"
      }
    }
  }
}
